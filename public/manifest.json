{
  "name": "Ollama KISS UI",
  "description": "A simple, stupid UI for Ollama. Keep It Simple, Stupid (KISS).",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_icon": "kiss.png"
  },
  "permissions": ["storage", "declarativeNetRequest"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://*/*", "http://*/*"]
}
